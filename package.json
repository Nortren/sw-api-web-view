{
  "name": "sw-api-web-view",
  "private": true,
  "version": "1.0.0",
  "description": "Pages with a managed table by swapi",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint ./src --fix --color",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "checking:dependencies": "depcheck"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/platform-express": "^9.4.2",
    "@next/eslint-plugin-next": "^13.4.4",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "antd": "^5.5.2",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.4",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.1.3",
    "next": "^12.1.5",
    "nock": "^13.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "redux": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "jest-styled-components": "^7.1.1",
    "lru-cache": "^9.1.2",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "depcheck": "^1.4.3",
    "@types/testing-library__react": "^10.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/cookie-parser": "^1.4.3",
    "@babel/preset-typescript": "^7.21.5",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/testing": "^9.4.2",
    "@types/express": "^4.17.17",
